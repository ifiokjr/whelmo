{
  "private": true,
  "name": "Whelmo",
  "version": "1.0.0-alpha.1",
  "workspaces": {
    "nohoist": [
      "react-native",
      "react-native/**",
      "**"
    ]
  },
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=\"release\"",
    "android:staging": "react-native run-android --variant=\"staging\"",
    "cd": "cd /",
    "codepush:android:prod": "appcenter codepush release-react -a KickJump/Whelmo-Android -m -d Production",
    "codepush:android:staging": "appcenter codepush release-react -a KickJump/Whelmo-Android -m -d Staging",
    "codepush:ios:prod": "appcenter codepush release-react -a KickJump/Whelmo-iOS -m -d Production",
    "codepush:ios:staging": "appcenter codepush release-react -a KickJump/Whelmo-iOS -m -d Staging",
    "codepush:rollback:android": "appcenter codepush rollback -a KickJump/Whelmo-Android Production",
    "codepush:rollback:ios": "appcenter codepush rollback -a KickJump/Whelmo-iOS Production",
    "ios": "react-native run-ios",
    "pod:install": "cd ios && bundle exec pod install --repo-update",
    "pod:update": "cd ios && bundle exec pod update",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "appcenter": "1.5.0",
    "appcenter-analytics": "1.5.0",
    "appcenter-crashes": "1.5.0",
    "native-base": "2.4.3",
    "ramda": "0.25.0",
    "react": "16.3.2",
    "react-intl": "2.4.0",
    "react-native": "0.55.3",
    "react-native-code-push": "5.3.2",
    "react-native-config": "0.11.5",
    "react-native-firebase": "4.0.6",
    "react-native-gesture-handler": "1.0.0-alpha.43",
    "react-native-i18n": "2.0.12",
    "react-native-keyboard-manager": "4.0.13-7",
    "react-native-keychain": "3.0.0-rc.3",
    "react-native-share": "1.0.27",
    "react-native-splash-screen": "3.0.6",
    "react-native-svg": "6.3.1",
    "react-native-vector-icons": "4.6.0",
    "react-navigation": "1.5.11",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@types/react": "16.3.13",
    "@types/react-native": "0.55.8",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "code-push-travis-cli": "1.2.0",
    "jest": "22.4.3",
    "react-native-typescript-transformer": "1.2.5",
    "react-test-renderer": "16.3.2"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
