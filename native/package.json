{
  "private": true,
  "name": "Whelmo",
  "version": "1.0.0-alpha.1",
  "workspaces": {
    "nohoist": [
      "react-native",
      "react-native/**",
      "**"
    ]
  },
  "scripts": {
    "android": "react-native run-android --appId com.whelmo.dev",
    "android:release": "react-native run-android --appId com.whelmo.app --variant=release",
    "android:staging": "react-native run-android --appId com.whelmo.staging --variant=staging",
    "cd": "cd /",
    "codepush:android:prod": "appcenter codepush release-react -a KickJump/Whelmo-Android -m -d Production",
    "codepush:android:staging": "appcenter codepush release-react -a KickJump/Whelmo-Android -m -d Staging",
    "codepush:ios:prod": "appcenter codepush release-react -a KickJump/Whelmo-iOS -m -d Production",
    "codepush:ios:staging": "appcenter codepush release-react -a KickJump/Whelmo-iOS -m -d Staging",
    "codepush:rollback:android": "appcenter codepush rollback -a KickJump/Whelmo-Android Production",
    "codepush:rollback:ios": "appcenter codepush rollback -a KickJump/Whelmo-iOS Production",
    "postinstall": "rndebugger-open",
    "ios": "react-native run-ios",
    "lint": "tslint -c ../tslint.json -p tsconfig.json",
    "pod:install": "cd ios && bundle exec pod install --repo-update",
    "pod:update": "cd ios && bundle exec pod update",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@types/color": "3.0.0",
    "@types/lodash": "4.14.108",
    "@whelmo/ui": "1.0.0-alpha.1",
    "appcenter": "1.5.0",
    "appcenter-analytics": "1.5.0",
    "appcenter-crashes": "1.5.0",
    "base-64": "0.1.0",
    "color": "3.0.0",
    "crypto-js": "3.1.9-1",
    "date-fns": "2.0.0-alpha.7",
    "es6-symbol": "3.1.1",
    "intl": "1.2.5",
    "lodash": "4.17.10",
    "mobx": "4.2.1",
    "mobx-persist": "0.4.1",
    "mobx-react": "5.1.2",
    "mobx-remotedev": "^0.2.8",
    "native-base": "2.4.4",
    "react": "16.3.2",
    "react-intl": "2.4.0",
    "react-native": "0.55.4",
    "react-native-autocomplete-input": "3.5.0",
    "react-native-blur": "3.2.2",
    "react-native-camera": "1.1.2",
    "react-native-code-push": "5.3.3",
    "react-native-config": "0.11.5",
    "react-native-fetch-blob": "0.10.8",
    "react-native-firebase": "4.1.0",
    "react-native-gesture-handler": "1.0.0-alpha.43",
    "react-native-google-analytics-bridge": "5.6.3",
    "react-native-htmlview": "^0.12.1",
    "react-native-i18n": "2.0.12",
    "react-native-indicators": "^0.13.0",
    "react-native-keyboard-manager": "4.0.13-7",
    "react-native-keychain": "3.0.0-rc.3",
    "react-native-linear-gradient": "2.4.0",
    "react-native-share": "1.0.27",
    "react-native-splash-screen": "3.0.6",
    "react-native-status-bar-height": "2.0.0",
    "react-native-svg": "6.3.1",
    "react-native-vector-icons": "4.6.0",
    "react-native-webview-bridge-updated": "1.0.9",
    "react-native-zss-rich-text-editor": "^1.1.0",
    "react-navigation": "1.5.12",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "@types/crypto-js": "3.1.39",
    "@types/react": "16.3.14",
    "@types/react-native": "0.55.12",
    "@types/react-native-fetch-blob": "0.10.4",
    "@types/react-navigation": "1.5.11",
    "babel-jest": "22.4.3",
    "babel-plugin-date-fns": "0.1.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-ramda": "1.6.1",
    "babel-plugin-transform-inline-environment-variables": "0.4.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-stage-0": "6.24.1",
    "code-push-travis-cli": "1.2.0",
    "colors": "1.2.5",
    "jest": "22.4.3",
    "react-native-debugger-open": "^0.3.17",
    "react-native-typescript-transformer": "1.2.5",
    "react-test-renderer": "16.3.2",
    "ts-jest": "22.4.5",
    "tslint-eslint-rules": "5.2.0",
    "tslint-react": "3.6.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  }
}
